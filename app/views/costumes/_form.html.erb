<div class="form-container">
 <h2 class="header">Add a new Costume</h2>
   <%= simple_form_for costume do |f| %>
     <%= f.input :photos, as: :file, input_html: { multiple: true } %>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :price %>
    <%= f.submit %>
     <% end %>

<%= image_tag "Marilyn.png", alt:"Marilyn", height:"400px", width:"400px", class:"lookhere" %>
</div>
