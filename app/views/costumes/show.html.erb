<h2>Name:<%= @costume.name %></h2>
<p>Description:<%= @costume.description %></p>
<p>Price:<%= @costume.price %></p>
<%= link_to "Check All Costumes", costumes_path %>
<% @costume.photos.each do |photo| %>
  <%= cl_image_tag photo.key, height: 300, width: 400, crop: :fill %>
<% end %>
<%= link_to "+Add a new Costume", costumes_new_path %>
